<header class="main-content__header flex place-content-between flex justify-between items-center mb-4 px-4 shadow-md py-4">
 <h4>Organization Name: <%= @account.name %></h4>
</header>
<div class="container mx-auto px-4">
  <%#= @account_users.count %>
  <div class="overflow-x-auto">
    <table class="border border-collapse mt-5">
      <thead>
        <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
      <% if @account_users.present? %>
          <% @account_users.each do |account_user| %>
            <tr>
              <td><%= account_user.user.first_name %></td>
              <td><%= account_user.user.last_name %></td>
              <td><%= account_user.user.email %></td>
            </tr>
          <% end %>
        <% else %>
        <tr>
          <td colspan="3">No Users found</td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
  
